{
  "name": "Blue Angels",
  "extends": "base",
  "inspiration": {
    "group": "US Navy Flight Demonstration Squadron",
    "palette": [
      "#001F3F",
      "#FFD700",
      "#EAF0F8"
    ],
    "notes": "Deep navy hulls with gold trim; keep high contrast for cockpits and bright skies."
  },
  "contrastTargets": {
    "Theme.Toolbar.Text|Theme.Toolbar.Background": 7.5,
    "Theme.Button.Primary.Text|Theme.Button.Primary.Background": 4.5,
    "Theme.Button.Secondary.Text|Theme.Button.Secondary.Background": 4.5
  },
  "tokens": {
    "Theme.Toolbar.Background": "#001F3F",
    "Theme.Surface.Primary": "#001F3F",
    "Theme.Button.Secondary.Background": "#001F3F",
    "Theme.Button.Secondary.HoverBackground": "#002B66",
    "Theme.Button.Secondary.PressedBackground": "#001433",
    "Theme.Button.Icon.Foreground": "#FFD700",
    "Theme.Button.Icon.HoverForeground": "#FFEA70",
    "Theme.DataGrid.HeaderBackground": "#001F3F",
    "Theme.DataGrid.HeaderText": "#FFFFFF",
    "Theme.Status.Info": "#1E90FF",
    "Theme.Template.Blue": "#1E90FF",
    "Theme.Template.Purple": "#6A0DAD"
  }
}
