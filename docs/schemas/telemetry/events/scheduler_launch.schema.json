{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://statetrace.local/schemas/telemetry/events/scheduler_launch.schema.json",
  "title": "SchedulerLaunch Event",
  "description": "Emitted when the scheduler launches a new runspace",
  "type": "object",
  "required": ["EventName", "Timestamp"],
  "properties": {
    "EventName": {
      "const": "SchedulerLaunch",
      "description": "Event type identifier"
    },
    "Timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp when event was emitted"
    },
    "RunspaceId": {
      "type": "string",
      "description": "Unique identifier for the runspace"
    },
    "TargetSite": {
      "type": "string",
      "description": "Target site for this runspace"
    },
    "HostBatchSize": {
      "type": "integer",
      "minimum": 1,
      "description": "Number of hosts in the batch"
    }
  },
  "additionalProperties": true
}
